{
  "name": "noflow",
  "version": "0.1.0",
  "description": "A minimal router for the future.",
  "main": "index.js",
  "scripts": {
    "no": "no",
    "test": "no test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ysmood/noflow"
  },
  "keywords": [
    "api",
    "app",
    "router",
    "web",
    "promise",
    "async",
    "await",
    "es7"
  ],
  "author": "http://ysmood.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysmood/flow/issues"
  },
  "homepage": "https://github.com/ysmood/flow",
  "dependencies": {
    "yaku": "latest"
  },
  "devDependencies": {
    "eslint": "1.3.0",
    "nokit": "latest",
    "babel": "latest",
    "babel-eslint": "latest",
    "body-parser": "1.13.2",
    "path-to-regexp": "^1.2.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "indent": [2, 4],
      "quotes": [2, "double"],
      "linebreak-style": [2, "unix"],
      "semi": [2, "always"],
      "no-trailing-spaces": [2, { "skipBlankLines": true }],
      "space-before-function-paren": [2, "always"],
      "eqeqeq": [2, "allow-null"],
      "no-console": [0]
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  }
}
