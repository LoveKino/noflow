{
  "rules": {
    "indent": [2, 4],
    "quotes": [2, "double"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "no-trailing-spaces": [2, { "skipBlankLines": true }],
    "space-before-function-paren": [2, "always"],
    "eqeqeq": [2, "allow-null"],
    "no-console": [0]
  },
  "env": {
    "node": true
  },
  "extends": "eslint:recommended"
}